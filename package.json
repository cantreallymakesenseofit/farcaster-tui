{
  "name": "farcaster-tui",
  "version": "0.2.1",
  "description": "A terminal-based Farcaster client",
  "type": "module",
  "bin": {
    "farcaster-tui": "bin/cli.cjs"
  },
  "scripts": {
    "start": "bun src/index.tsx",
    "dev": "bun --watch src/index.tsx",
    "build": "bun build src/index.tsx --compile --outfile farcaster-tui",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@farcaster/hub-nodejs": "^0.15.9",
    "@inkjs/ui": "^2.0.0",
    "ink": "^6.0.0",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.0.0",
    "typescript": "^5.8.0"
  },
  "keywords": [
    "farcaster",
    "tui",
    "terminal",
    "cli",
    "typescript",
    "bun"
  ],
  "author": "backmeupplz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/backmeupplz/farcaster-tui.git"
  }
}
